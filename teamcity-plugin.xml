<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>vsphere</name>
    <display-name>VMware vSphere</display-name>
    <version>@Plugin_Version@</version>
    <description>Runs cloud agents on VMware vCenter and vSphere</description>
    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>
